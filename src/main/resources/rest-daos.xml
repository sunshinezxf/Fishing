<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
    <bean id="fishDao" class="fishing.sunshine.dao.impl.FishDaoImpl">
        <property name="sqlSession" ref="sqlSession"/>
    </bean>

    <bean id="pondTypeDao" class="fishing.sunshine.dao.impl.PondTypeDaoImpl">
        <property name="sqlSession" ref="sqlSession"/>
    </bean>

    <bean id="contractorDao" class="fishing.sunshine.dao.impl.ContractorDaoImpl">
        <property name="sqlSession" ref="sqlSession"/>
    </bean>

    <bean id="fishPondDao" class="fishing.sunshine.dao.impl.FishPondDaoImpl">
        <property name="sqlSession" ref="sqlSession"/>
    </bean>
</beans>
